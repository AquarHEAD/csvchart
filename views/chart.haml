.row
  .col-md-8.col-md-offset-2
    %h1
      = @filename
      %small
        %a.btn.btn-info{href: "/"} Back to Index
    %form#choices.form-inline{action: "javascript:void(0);"}
      .checkbox
        %label
          %input{type: "checkbox", name: "frame", checked: "checked", id: "check_frame"}
          Frame
      .checkbox
        %label
          %input{type: "checkbox", name: "gt", checked: "checked", id: "check_gt"}
          Game Thread
      .checkbox
        %label
          %input{type: "checkbox", name: "rt", checked: "checked", id: "check_rt"}
          Render Thread
      .checkbox
        %label
          %input{type: "checkbox", name: "temp", checked: "checked", id: "check_temp"}
          Temperature
      .checkbox
        %label
          %input{type: "checkbox", name: "power", checked: "checked", id: "check_power"}
          Power Level
      %button.btn.btn-default{type: "submit", id: "replot"} Replot
.row
  .col-md-10
    #chart{style: "width:100%; height:400px; margin: 20 auto;"}
  .col-md-2
    %h4#eventstitle Events
    .well#eventslist
.row
  .col-md-12
    #overview{style: "width:100%; height: 100px; margin: 20 auto;"}
- if @table_data
  .row
    .col-md-12
      != @table_data
:javascript
  app(#{@frame_data}, #{@gt_data}, #{@rt_data}, #{@temp_data}, #{@power_data}, "#chart", "#overview", "#eventslist", "#eventstitle", #{@events});
